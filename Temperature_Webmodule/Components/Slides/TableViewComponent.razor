<link rel="stylesheet" href="css/myown/TableView.css">


<div class="table-responsive">
    <table class="table table-striped table-hover">
        <thead>
            <tr>
                <th>Tidpunkt</th>
                <th>Temperatur (°C)</th>
            </tr>
        </thead>
        <tbody>
            @if (unitData != null)
            {
                @for (int i = unitData[0].UnitDataPT.Count - 1; 0 < i; i--)
                {
                    <tr>
                        <td>@unitData[0].UnitDataPT[i].DataDateTime.ToShortTimeString()</td>
                        <td>@unitData[0].UnitDataPT[i].DataPTValue</td>
                    </tr>
                }
            }
        </tbody>
    </table>
</div>

@code {
    [CascadingParameter]
    public TempraturModul.Models.TempratureDTO[] unitData { get; set; }

}
